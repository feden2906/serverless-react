{
  "name": "serverless-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env IS_OFFLINE=1 concurrently --kill-others \"npm run start:devserver\" \"npm run start:offline\"",
    "start:devserver": "webpack serve --config webpack.browser.config.js --mode development",
    "start:offline": "sls offline start",
    "build:serverless": "sls package",
    "build:browser": "webpack --config webpack.browser.config.js --mode production",
    "deploy": "sls deploy",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "serverless-dotenv-plugin": "^4.0.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/preset-typescript": "^7.18.6",
    "@types/aws-lambda": "^8.10.108",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/babel-core": "^6.25.7",
    "@types/concurrently": "^7.0.0",
    "@types/file-loader": "^5.0.1",
    "@types/jest": "^29.2.1",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/serverless": "^3.12.8",
    "@types/source-map-support": "^0.5.6",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.2",
    "aws-sdk": "^2.1247.0",
    "babel-jest": "^29.2.2",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.2",
    "mini-css-extract-plugin": "^2.6.1",
    "null-loader": "^4.0.1",
    "prettier": "^2.7.1",
    "serverless": "^3.24.0",
    "serverless-export-env": "^2.2.0",
    "serverless-offline": "^11.2.3",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-s3-deploy": "^0.10.1",
    "serverless-webpack": "^5.10.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-stats-plugin": "^1.1.1"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  }
}
